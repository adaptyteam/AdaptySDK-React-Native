{
  "$schema": "https://json.schemastore.org/package",
  "name": "react-native-adapty",
  "version": "2.4.7",
  "description": "Adapty React Native SDK",
  "license": "MIT",
  "author": "Vanya Dorofeyev <DivanDoesApps@gmail.com> (https://github.com/divanc)",
  "keywords": [
    "react-native",
    "adapty",
    "purchases",
    "in-apps",
    "subscriptions"
  ],
  "repository": "https://github.com/adaptyteam/AdaptySDK-React-Native",
  "homepage": "https://github.com/adaptyteam/AdaptySDK-React-Native#readme",
  "bugs": {
    "url": "https://github.com/adaptyteam/AdaptySDK-React-Native/issues"
  },
  "files": [
    "lib/dist/",
    "lib/ios/",
    "!lib/ios/build",
    "!lib/ios/RNAdapty.xcodeproj/project.xcworkspace",
    "!lib/ios/RNAdapty.xcodeproj/xcuserdata",
    "react-native-adapty.podspec",
    "lib/android/",
    "!lib/android/build",
    "!lib/android/gradle",
    "!lib/android/gradlew*",
    "!lib/android/local.properties",
    "react-native.config.js",
    "!**/.*"
  ],
  "main": "lib/dist/index.js",
  "scripts": {
    "build": "rm -rf lib/dist && tsc --project tsconfig.build.json",
    "clean": "./scripts/clean.sh",
    "update-versions": "./scripts/update_versions.sh",
    "prepack": "npm run update-versions && npm run build",
    "prepublishOnly": "npm run prepack"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "7.10.4",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.14.2",
    "@types/react": "^16.13.1",
    "@types/react-native": "^0.63.0",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "bun-types": "^0.5.7",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.3.1",
    "prettier": "2.7.1",
    "prompt-sync": "^4.2.0",
    "react": "16.13.1",
    "react-native": "0.63.0",
    "react-native-config": "^1.3.3",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.23",
    "typescript": "5.1.3"
  },
  "peerDependencies": {
    "react-native": ">= 0.60.0"
  },
  "peerDependenciesMeta": {}
}
